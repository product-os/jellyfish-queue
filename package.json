{
  "name": "@balena/jellyfish-queue",
  "version": "1.0.105",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-queue.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "Queue library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint-fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && jellycheck && deplint && depcheck --ignore-bin-package --ignores=winston-transport,@types/jest",
    "lint-fix": "balena-lint --fix lib test",
    "unit": "jest \"lib/.*\\.spec\\.ts\"",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run unit",
    "test-integration": "jest ./test/integration/**/*.spec.ts",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.29",
    "@balena/jellyfish-environment": "^4.1.22",
    "@balena/jellyfish-logger": "^2.1.81",
    "@balena/jellyfish-metrics": "^1.0.231",
    "@graphile/logger": "^0.2.0",
    "bluebird": "^3.7.2",
    "graphile-worker": "^0.11.1",
    "is-uuid": "^1.0.2",
    "jest-cli": "^26.6.3",
    "lodash": "^4.17.21",
    "typed-error": "^3.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-config": "^1.0.6",
    "@balena/jellyfish-core": "^2.13.0",
    "@balena/jellyfish-types": "^0.5.35",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.35",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "catch-uncommitted": "^2.0.0",
    "depcheck": "^1.4.1",
    "deplint": "^1.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "simple-git-hooks": "^2.4.1",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
