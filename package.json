{
  "name": "@balena/jellyfish-queue",
  "version": "1.0.374",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-queue.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Queue library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint && depcheck --ignore-bin-package --ignores=winston-transport,@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest \"lib/.*\\.spec\\.ts\"",
    "test:integration": "jest ./test/integration/**/*.spec.ts -- --verbose",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.99",
    "@balena/jellyfish-environment": "^5.2.0",
    "@balena/jellyfish-logger": "^3.0.156",
    "@balena/jellyfish-metrics": "^1.0.424",
    "@graphile/logger": "^0.2.0",
    "bluebird": "^3.7.2",
    "graphile-worker": "^0.12.1",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.21",
    "typed-error": "^3.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.1",
    "@balena/jellyfish-core": "^8.1.0",
    "@balena/jellyfish-types": "^1.2.2",
    "@balena/lint": "^6.2.0",
    "@types/bluebird": "^3.5.36",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^8.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-12-03T01:45:02.222Z"
  }
}
