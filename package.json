{
  "name": "@balena/jellyfish-queue",
  "version": "4.0.13",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-queue.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Queue library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest \"lib/.*\\.spec\\.ts\"",
    "test:integration": "jest ./test/integration -- --verbose",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.11",
    "@balena/jellyfish-core": "^14.0.11",
    "@balena/jellyfish-environment": "^6.0.13",
    "@balena/jellyfish-logger": "^4.0.27",
    "@balena/jellyfish-metrics": "^2.0.35",
    "@graphile/logger": "^0.2.0",
    "graphile-worker": "^0.12.2",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.21",
    "typed-error": "^3.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-types": "^2.0.0",
    "@balena/lint": "^6.2.0",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/pg": "^8.6.4",
    "@types/uuid": "^8.3.4",
    "deplint": "^1.1.3",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.1",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-01-24T19:53:18.074Z"
  }
}
