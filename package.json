{
  "name": "@balena/jellyfish-queue",
  "version": "0.0.321",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-queue.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Queue library for Jellyfish",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/*.js > README.md",
    "postinstall": "if [ -z \"$CI\" ] && [ -d \".git\" ]; then husky install; fi"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.0.51",
    "@balena/jellyfish-environment": "^2.3.24",
    "@balena/jellyfish-logger": "^0.0.204",
    "@balena/jellyfish-metrics": "^0.1.18",
    "@balena/jellyfish-uuid": "^0.0.31",
    "bluebird": "^3.7.2",
    "graphile-worker": "^0.8.1",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.20",
    "typed-errors": "^1.1.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-action-library": "^6.0.4",
    "@balena/jellyfish-core": "^0.2.6",
    "ava": "^3.15.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.3.1",
    "deplint": "^1.1.2",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.0.7",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^5.0.6",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.5.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc",
      "git add README.md"
    ]
  }
}
