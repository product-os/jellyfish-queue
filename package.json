{
  "name": "@balena/jellyfish-queue",
  "version": "1.0.221",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-queue.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Queue library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && jellycheck && deplint && depcheck --ignore-bin-package --ignores=winston-transport,@types/jest",
    "lint-fix": "balena-lint --fix lib test",
    "unit": "jest \"lib/.*\\.spec\\.ts\"",
    "integration": "jest ./test/integration/**/*.spec.ts -- --verbose",
    "test": "npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.64",
    "@balena/jellyfish-environment": "^4.3.16",
    "@balena/jellyfish-logger": "^3.0.50",
    "@balena/jellyfish-metrics": "^1.0.321",
    "@graphile/logger": "^0.2.0",
    "bluebird": "^3.7.2",
    "graphile-worker": "^0.11.3",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.21",
    "typed-error": "^3.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-config": "^1.3.3",
    "@balena/jellyfish-core": "^5.0.0",
    "@balena/jellyfish-types": "^0.7.0",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "simple-git-hooks": "^2.5.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.21.3",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^5.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
